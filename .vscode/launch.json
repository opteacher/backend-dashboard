    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Frontend",
            "program": "${workspaceFolder}/frontend/app.js"
        },
        {
            "name": "Backend",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/backend/cmd/main.go",
            "env": {
                "http_proxy": "http://127.0.0.1:63617"
            },
            "args": [
                "-conf", "${workspaceFolder}/backend/configs/"
            ]
        }
    ]
}